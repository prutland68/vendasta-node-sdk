{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": [
        "src"
    ],
    "exclude": [
        "node_modules"
    ],
    "includePattern": "(\\.d)?\\.ts$"
  },
  "opts": {
    "package": "package.json",
    "readme": "README.md",
    "destination": "./docs/",
    "template": "node_modules/ts2jsdoc/template"
  },
  "plugins": [
      "node_modules/ts2jsdoc/plugin.js"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    }
  }
}